aView);\r\n    }\r\n\r\n    public void setSoundEffect(int newVal){\r\n        this.soundEffect = newVal;\r\n    }\r\n    public void setSoundVolume(int newVal){\r\n        this.soundVolume = newVal;\r\n    }\r\n\r\n    public int getSoundEffect(){\r\n        return soundEffect;\r\n    }\r\n\r\n    public int getSoundVolume(){\r\n        return soundVolume;\r\n    }\r\n}\r\n
